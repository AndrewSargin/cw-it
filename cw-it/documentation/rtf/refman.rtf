00215     QPrinter printer(QPrinter::HighResolution);\par
00216     printer.setOutputFormat(QPrinter::PdfFormat);\par
00217     printer.setOutputFileName(QDir::currentPath() + {\cf22 "/cw-it.pdf"});\par
00218 \par
00219     {\cf20 //\'D0\'9D\'D0\'B0\'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'B9\'D0\'BA\'D0\'B0 \'D0\'BF\'D0\'B0\'D1\'80\'D0\'B0\'D0\'BC\'D0\'B5\'D1\'82\'D1\'80\'D0\'BE\'D0\'B2 \'D1\'81\'D1\'82\'D1\'80\'D0\'B0\'D0\'BD\'D0\'B8\'D1\'86\'D1\'8B}\par
00220     printer.pageLayout().setMode(QPageLayout::FullPageMode);\par
00221     printer.setPageMargins(QMargins(0, 0, 0, 0));\par
00222     printer.setPageOrientation(QPageLayout::Orientation::Landscape);\par
00223 \par
00224     {\cf20 //\'D0\'A1\'D0\'BE\'D0\'B7\'D0\'B4\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'B4\'D0\'B8\'D0\'B0\'D0\'BB\'D0\'BE\'D0\'B3\'D0\'BE\'D0\'B2\'D0\'BE\'D0\'B5 \'D0\'BE\'D0\'BA\'D0\'BD\'D0\'BE \'D0\'BF\'D0\'B5\'D1\'87\'D0\'B0\'D1\'82\'D0\'B8 \'D0\'B8 \'D1\'83\'D1\'81\'D1\'82\'D0\'B0\'D0\'BD\'D0\'B0\'D0\'B2\'D0\'BB\'D0\'B8\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'BD\'D0\'B0\'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'B9\'D0\'BA\'D0\'B8 \'D0\'BF\'D1\'80\'D0\'B8\'D0\'BD\'D1\'82\'D0\'B5\'D1\'80\'D0\'B0}\par
00225     QPrintDialog printDialog(&printer, {\cf17 nullptr});\par
00226     printDialog.setWindowTitle({\cf22 "Print"});\par
00227     printDialog.setOptions(QAbstractPrintDialog::PrintToFile | QAbstractPrintDialog::PrintPageRange);\par
00228 \par
00229     {\cf19 if} (printDialog.exec() != QDialog::Accepted)\par
00230     \{\par
00231         {\cf19 return};\par
00232     \}\par
00233 \par
00234     {\cf20 //\'D0\'97\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8C \'D0\'B4\'D0\'B0\'D0\'BD\'D0\'BD\'D1\'8B\'D1\'85 \'D0\'B2 \'D0\'B4\'D0\'BE\'D0\'BA\'D1\'83\'D0\'BC\'D0\'B5\'D0\'BD\'D1\'82 \'D0\'B4\'D0\'BB\'D1\'8F \'D0\'B2\'D1\'8B\'D0\'B2\'D0\'BE\'D0\'B4\'D0\'B0}\par
00235     QString htmlTable;\par
00236     htmlTable = {\cf22 "<table border=1 cellspacing=1>\\n"}\par
00237                 {\cf22 "<caption>\'D0\'A3\'D1\'87\'D0\'B5\'D1\'82 \'D0\'B2\'D1\'8B\'D1\'87\'D0\'B8\'D1\'81\'D0\'BB\'D0\'B8\'D1\'82\'D0\'B5\'D0\'BB\'D1\'8C\'D0\'BD\'D0\'BE\'D0\'B9 \'D1\'82\'D0\'B5\'D1\'85\'D0\'BD\'D0\'B8\'D0\'BA\'D0\'B8 \'D0\'BD\'D0\'B0 \'D0\'B1\'D0\'B0\'D0\'BB\'D0\'B0\'D0\'BD\'D1\'81\'D0\'B5 \'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'B8\'D1\'82\'D0\'B5\'D0\'BB\'D1\'8C\'D0\'BD\'D0\'BE\'D0\'B9 \'D0\'BA\'D0\'BE\'D0\'BC\'D0\'BF\'D0\'B0\'D0\'BD\'D0\'B8\'D0\'B8</caption>\\n"}\par
00238                 {\cf22 "<tr>\\n"};\par
00239 \par
00240     {\cf19 for} ({\cf18 int} i = 0; i < (int) columnNames.size(); i++)\par
00241     \{\par
00242         htmlTable += {\cf22 "<th>"};\par
00243         htmlTable += columnNames[i].c_str();\par
00244         htmlTable += {\cf22 "</th>\\n"};\par
00245     \}\par
00246 \par
00247     htmlTable += {\cf22 "</tr>\\n"};\par
00248 \par
00249     std::map<int, Entry> data = fileHandler.getFileData(ui->tabWidget->currentIndex());\par
00250 \par
