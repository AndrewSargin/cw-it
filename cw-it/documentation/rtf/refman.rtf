00034 {\cf18 void} Chart::CreateChart(std::vector<float> price)\par
00035 \{\par
00036 \par
00037     QBarSet *dataSet = {\cf17 new} QBarSet(tr({\cf22 "Number of computers"}));\par
00038     QBarSeries *series = {\cf17 new} QBarSeries();\par
00039     QChart *chart = {\cf17 new} QChart();\par
00040     QChartView *chartView = {\cf17 new} QChartView(chart);\par
00041 \par
00042     QStringList barTitles;\par
00043 \par
00044     std::sort(price.begin(), price.end());\par
00045 \par
00046     {\cf18 int} barAmount = 1 + int(log2(price.size()));\par
00047 \par
00048     {\cf18 float} step = (price[price.size() - 1] - price[0]) / barAmount;\par
00049 \par
00050     {\cf19 for} ({\cf18 int} i = 0,  j = 0; i < barAmount; i++)\par
00051     \{\par
00052         {\cf18 int} barData = 0;\par
00053         QString left = QString::number(price[0] + i * step);\par
00054         QString right = QString::number(price[0] + (i+1) * step);\par
00055         barTitles.append(left + {\cf22 " - "} + right);\par
00056 \par
00057         {\cf19 while} ((price[j] <= (price[0] + (i + 1) * step) && j < ({\cf18 int}) price.size()))\par
00058         \{\par
00059             barData +=1;\par
00060             j++;\par
00061         \}\par
00062 \par
00063         *dataSet << barData;\par
00064     \}\par
00065 \par
00066     series->append(dataSet);\par
00067     chart->addSeries(series);\par
00068 \par
00069     QBarCategoryAxis *axisX = {\cf17 new} QBarCategoryAxis();\par
00070     axisX->append(barTitles);\par
00071 \par
00072     chart->addAxis(axisX, Qt::AlignBottom);\par
00073     series->attachAxis(axisX);\par
00074 \par
00075     QValueAxis *axisY = {\cf17 new} QValueAxis();\par
00076     chart->addAxis(axisY, Qt::AlignLeft);\par
00077     series->attachAxis(axisY);\par
00078 \par
00079     chart->setTitle(tr({\cf22 "Number of computers depending on price"}));\par
00080 \par
00081     this->setCentralWidget(chartView);\par
00082     this->resize(1000, 600);\par
00083 \par
00084 \}\par
}
