00032 {\cf18 void} Validator::DateInput(QTableWidgetItem *item, std::map<std::string, std::string> *currentLine)\par
00033 \{\par
00034 \par
00035     {\cf20 //\'D0\'9F\'D1\'80\'D0\'B5\'D0\'BE\'D0\'B1\'D1\'80\'D0\'B0\'D0\'B7\'D0\'BE\'D0\'B2\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'B4\'D0\'B0\'D0\'BD\'D0\'BD\'D1\'8B\'D0\'B5 \'D0\'B2 \'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'BA\'D0\'BE\'D0\'B2\'D1\'8B\'D0\'B9 \'D1\'82\'D0\'B8\'D0\'BF}\par
00036     std::string dateStr = item->text().toStdString();\par
00037     std::string writeDate;\par
00038     std::string ddStr, mmStr, yyyyStr;\par
00039     {\cf18 int} ddNum, mmNum, yyyyNum;\par
00040 \par
00041     {\cf20 //\'D0\'95\'D1\'81\'D0\'BB\'D0\'B8 \'D0\'B4\'D0\'BB\'D0\'B8\'D0\'BD\'D0\'B0 \'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'BA\'D0\'B8 \'D0\'B4\'D0\'B0\'D1\'82\'D1\'8B \'D0\'BD\'D0\'B5 8, \'D0\'B8 \'D0\'BD\'D0\'B5 10, \'D0\'B2\'D0\'BE\'D1\'81\'D1\'81\'D1\'82\'D0\'B0\'D0\'BD\'D0\'B0\'D0\'B2\'D0\'BB\'D0\'B8\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'B4\'D0\'B0\'D0\'BD\'D0\'BD\'D1\'8B\'D0\'B5 \'D0\'B2 \'D1\'8F\'D1\'87\'D0\'B5\'D0\'B9\'D0\'BA\'D0\'B5}\par
00042     {\cf19 if} (dateStr.length() != 8 && dateStr.length() != 10)\par
00043     \{\par
00044         item->setText(QString::fromStdString(currentLine->at({\cf22 "DateOfPurchase"})));\par
00045         {\cf19 return};\par
00046     \}\par
00047     {\cf19 else} \{\par
00048         {\cf20 //\'D0\'95\'D1\'81\'D0\'BB\'D0\'B8 \'D0\'B4\'D0\'BB\'D0\'B8\'D0\'BD\'D0\'B0 \'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'BA\'D0\'B8 \'D1\'80\'D0\'B0\'D0\'B2\'D0\'BD\'D0\'B0 8}\par
00049         {\cf19 if} (dateStr.length() == 8)\par
00050         \{\par
00051             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'B4\'D0\'BD\'D1\'8F \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'BF\'D0\'B5\'D1\'80\'D0\'B2\'D1\'8B\'D0\'B5 2 \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D0\'B0}\par
00052             ddStr = dateStr.substr(0, 2);\par
00053             ddNum = std::stoi(ddStr);\par
00054 \par
00055             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'BC\'D0\'B5\'D1\'81\'D1\'8F\'D1\'86\'D0\'B0 \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC 3 \'D0\'B8 4 \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D1\'8B}\par
00056             mmStr = dateStr.substr(2, 2);\par
00057             mmNum = std::stoi(mmStr);\par
00058 \par
00059             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'B3\'D0\'BE\'D0\'B4\'D0\'B0 \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D1\'8B \'D1\'81 5 \'D0\'BF\'D0\'BE 8}\par
00060             yyyyStr = dateStr.substr(4, 4);\par
00061             yyyyNum = std::stoi(yyyyStr);\par
00062         \}\par
00063 \par
00064         {\cf20 //\'D0\'95\'D1\'81\'D0\'BB\'D0\'B8 \'D0\'B4\'D0\'BB\'D0\'B8\'D0\'BD\'D0\'B0 \'D1\'81\'D1\'82\'D1\'80\'D0\'BE\'D0\'BA\'D0\'B8 \'D1\'80\'D0\'B0\'D0\'B2\'D0\'BD\'D0\'B0 10}\par
00065         {\cf19 else}\par
00066         \{\par
00067             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'B4\'D0\'BD\'D1\'8F \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'BF\'D0\'B5\'D1\'80\'D0\'B2\'D1\'8B\'D0\'B5 2 \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D0\'B0}\par
00068             ddStr = dateStr.substr(0, 2);\par
00069             ddNum = std::stoi(ddStr);\par
00070 \par
00071             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'BC\'D0\'B5\'D1\'81\'D1\'8F\'D1\'86\'D0\'B0 \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC 4 \'D0\'B8 5 \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D0\'B0}\par
00072             mmStr = dateStr.substr(3, 2);\par
00073             mmNum = std::stoi(mmStr);\par
00074 \par
00075             {\cf20 //\'D0\'92 \'D0\'BA\'D0\'B0\'D1\'87\'D0\'B5\'D1\'81\'D1\'82\'D0\'B2\'D0\'B5 \'D0\'B3\'D0\'BE\'D0\'B4\'D0\'B0 \'D0\'B7\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8B\'D0\'B2\'D0\'B0\'D0\'B5\'D0\'BC \'D1\'81\'D0\'B8\'D0\'BC\'D0\'B2\'D0\'BE\'D0\'BB\'D1\'8B \'D1\'81 7 \'D0\'BF\'D0\'BE 10}\par
00076             yyyyStr = dateStr.substr(6, 4);\par
00077             yyyyNum = std::stoi(yyyyStr);\par
00078         \}\par
00079 \par
00080         {\cf20 //\'D0\'9F\'D0\'BE\'D0\'BB\'D1\'83\'D1\'87\'D0\'B0\'D0\'B5\'D0\'BC \'D0\'BD\'D1\'8B\'D0\'BD\'D0\'B5\'D1\'88\'D0\'BD\'D0\'B8\'D0\'B9 \'D0\'B3\'D0\'BE\'D0\'B4}\par
00081         time_t now = time(0);\par
00082         tm* ltm = localtime(&now);\par
00083         {\cf18 int} currentYear = 1900 + ltm->tm_year;\par
00084 \par
00085         {\cf20 //\'D0\'9F\'D1\'80\'D0\'BE\'D0\'B2\'D0\'B5\'D1\'80\'D0\'BA\'D0\'B0 \'D0\'B2\'D0\'B2\'D0\'B5\'D0\'B4\'D0\'B5\'D0\'BD\'D0\'BD\'D0\'BE\'D0\'B9 \'D0\'B4\'D0\'B0\'D1\'82\'D0\'B8 \'D0\'BD\'D0\'B0 \'D0\'BF\'D1\'80\'D0\'B0\'D0\'B2\'D0\'B4\'D0\'BE\'D0\'BF\'D0\'BE\'D0\'B4\'D0\'BE\'D0\'B1\'D0\'BD\'D0\'BE\'D1\'81\'D1\'82\'D1\'8C}\par
00086         {\cf19 if} (ddNum < 1 || ddNum > 31 || mmNum < 1 || mmNum > 12 ||\par
00087                 yyyyNum < 1990 || yyyyNum > currentYear)\par
00088         \{\par
00089             item->setText(QString::fromStdString(currentLine->at({\cf22 "DateOfPurchase"})));\par
00090             {\cf19 return};\par
00091         \}\par
00092     \}\par
00093 \par
00094     {\cf20 //\'D0\'97\'D0\'B0\'D0\'BF\'D0\'B8\'D1\'81\'D1\'8C \'D0\'B2\'D0\'B2\'D0\'B5\'D0\'B4\'D0\'B5\'D0\'BD\'D0\'BD\'D1\'8B\'D1\'85 \'D0\'B4\'D0\'B0\'D0\'BD\'D0\'BD\'D1\'8B\'D1\'85 \'D0\'B2 \'D1\'8F\'D1\'87\'D0\'B5\'D0\'B9\'D0\'BA\'D1\'83 \'D1\'82\'D0\'B0\'D0\'B1\'D0\'BB\'D0\'B8\'D1\'86\'D1\'8B}\par
00095     writeDate = ddStr + {\cf22 "."} + mmStr + {\cf22 "."} + yyyyStr;\par
00096     item->setText(QString::fromStdString(writeDate));\par
00097 \}\par
}
