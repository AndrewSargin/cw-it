00033 vector<vector<float>> ConvertChartData(std::map<int, Entry> table)\par
00034 \{\par
00035     vector<vector<float>> data;\par
00036 \par
00037     {\cf19 for} ({\cf17 auto} i = table.begin(); i != table.end(); i++)\par
00038     \{\par
00039         {\cf18 float} ramFloat = 0;\par
00040         {\cf18 float} memoryFloat = 0;\par
00041         {\cf18 float} costFloat = 0;\par
00042         {\cf18 string} ramStr = i->second.properties.at(entryProps[4]);\par
00043         {\cf19 if} (ramStr != {\cf22 ""})\par
00044             ramFloat = std::stof(SplitLine(ramStr, {\cf22 " "})[0]);\par
00045         {\cf19 else} {\cf19 continue};\par
00046 \par
00047         {\cf18 string} memoryStr = i->second.properties.at(entryProps[5]);\par
00048         {\cf19 if} (memoryStr != {\cf22 ""})\par
00049         \{\par
00050             vector<string> memoryStrVector = SplitLine(memoryStr, {\cf22 "+"});\par
00051             {\cf19 for} ({\cf18 int} j = 0; j < (int) memoryStrVector.size(); j++)\par
00052             \{\par
00053                 vector<string> memoryStrSubVector = SplitLine(memoryStrVector[j], {\cf22 " "});\par
00054 \par
00055                 {\cf19 if} (memoryStrSubVector[1] == {\cf22 "\'C3\'E1"})\par
00056                     memoryFloat += std::stof(memoryStrSubVector[0]);\par
00057                 {\cf19 else} memoryFloat += std::stof(memoryStrSubVector[0]) * 1024;\par
00058 \par
00059             \}\par
00060 \par
00061         \}\par
00062         {\cf19 else} {\cf19 continue};\par
00063 \par
00064         costFloat = std::stof(i->second.properties.at(entryProps[9]));\par
00065         vector<float> vector;\par
00066         vector.push_back(ramFloat);\par
00067         vector.push_back(memoryFloat);\par
00068         vector.push_back(costFloat);\par
00069 \par
00070         data.push_back(vector);\par
00071     \}\par
00072 \par
00073     {\cf19 return} data;\par
00074 \}\par
}
