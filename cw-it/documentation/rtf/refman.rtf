00215     {\cf20 //\'CD\'E0\'F1\'F2\'F0\'EE\'E9\'EA\'E0 \'EF\'E0\'F0\'E0\'EC\'E5\'F2\'F0\'EE\'E2 \'EF\'E5\'F7\'E0\'F2\'E8}\par
00216     QPrinter printer(QPrinter::HighResolution);\par
00217     printer.setOutputFormat(QPrinter::PdfFormat);\par
00218     printer.setOutputFileName(QDir::currentPath() + {\cf22 "/cw-it.pdf"});\par
00219 \par
00220     {\cf20 //\'CD\'E0\'F1\'F2\'F0\'EE\'E9\'EA\'E0 \'EF\'E0\'F0\'E0\'EC\'E5\'F2\'F0\'EE\'E2 \'F1\'F2\'F0\'E0\'ED\'E8\'F6\'FB}\par
00221     printer.pageLayout().setMode(QPageLayout::FullPageMode);\par
00222     printer.setPageMargins(QMargins(0, 0, 0, 0));\par
00223     printer.setPageOrientation(QPageLayout::Orientation::Landscape);\par
00224 \par
00225     {\cf20 //\'D1\'EE\'E7\'E4\'E0\'E5\'EC \'E4\'E8\'E0\'EB\'EE\'E3\'EE\'E2\'EE\'E5 \'EE\'EA\'ED\'EE \'EF\'E5\'F7\'E0\'F2\'E8 \'E8 \'F3\'F1\'F2\'E0\'ED\'E0\'E2\'EB\'E8\'E2\'E0\'E5\'EC \'ED\'E0\'F1\'F2\'F0\'EE\'E9\'EA\'E8 \'EF\'F0\'E8\'ED\'F2\'E5\'F0\'E0}\par
00226     QPrintDialog printDialog(&printer, {\cf17 nullptr});\par
00227     printDialog.setWindowTitle({\cf22 "Print"});\par
00228     printDialog.setOptions(QAbstractPrintDialog::PrintToFile | QAbstractPrintDialog::PrintPageRange);\par
00229 \par
00230     {\cf19 if} (printDialog.exec() != QDialog::Accepted)\par
00231     \{\par
00232         {\cf19 return};\par
00233     \}\par
00234 \par
00235     {\cf20 //\'C7\'E0\'EF\'E8\'F1\'FC \'E4\'E0\'ED\'ED\'FB\'F5 \'E2 \'E4\'EE\'EA\'F3\'EC\'E5\'ED\'F2 \'E4\'EB\'FF \'E2\'FB\'E2\'EE\'E4\'E0}\par
00236     QString htmlTable;\par
00237     htmlTable = {\cf22 "<table border=1 cellspacing=1>\\n"}\par
00238                 {\cf22 "<caption>\'D3\'F7\'E5\'F2 \'E2\'FB\'F7\'E8\'F1\'EB\'E8\'F2\'E5\'EB\'FC\'ED\'EE\'E9 \'F2\'E5\'F5\'ED\'E8\'EA\'E8 \'ED\'E0 \'E1\'E0\'EB\'E0\'ED\'F1\'E5 \'F1\'F2\'F0\'EE\'E8\'F2\'E5\'EB\'FC\'ED\'EE\'E9 \'EA\'EE\'EC\'EF\'E0\'ED\'E8\'E8</caption>\\n"}\par
00239                 {\cf22 "<tr>\\n"};\par
00240 \par
00241     {\cf19 for} ({\cf18 int} i = 0; i < (int) columnNames.size(); i++)\par
00242     \{\par
00243         htmlTable += {\cf22 "<th>"};\par
00244         htmlTable += columnNames[i].c_str();\par
00245         htmlTable += {\cf22 "</th>\\n"};\par
00246     \}\par
00247 \par
00248     htmlTable += {\cf22 "</tr>\\n"};\par
00249 \par
00250     std::map<int, Entry> data = fileHandler.getFileData(ui->tabWidget->currentIndex());\par
00251 \par
00252     {\cf19 for} ({\cf17 auto} i = data.begin(); i != data.end(); i++)\par
00253     \{\par
00254        htmlTable += {\cf22 "<tr>"};\par
00255 \par
00256        {\cf19 for}({\cf18 int} j = 0; j < 11; j++)\par
00257        \{\par
00258            htmlTable += {\cf22 "<td>"};\par
00259            htmlTable += i->second.properties.at(entryProps[j]).c_str();\par
00260            htmlTable += {\cf22 "</td>"};\par
00261        \}\par
00262 \par
00263        htmlTable += {\cf22 "</tr>\\n"};\par
00264     \}\par
00265 \par
00266     htmlTable += {\cf22 "</table>\\n<br>\\n"};\par
00267 \par
00268     QTextDocument document;\par
00269     document.setHtml(htmlTable);\par
00270 \par
00271     QTextBrowser textBrowser;\par
00272     textBrowser.setDocument(&document);\par
00273 \par
00274     {\cf20 //\'CF\'E5\'F7\'E0\'F2\'FC \'E4\'EE\'EA\'F3\'EC\'E5\'ED\'F2\'E0}\par
00275     textBrowser.print(&printer);\par
00276 \par
00277     {\cf19 return};\par
00278 \}\par
