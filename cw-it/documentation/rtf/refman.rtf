00040 {\cf18 void} Chart::CreateChart(std::vector<std::vector<float>> data)\par
00041 \{\par
00042     QBarSeries *series = {\cf17 new} QBarSeries;\par
00043 \par
00044     std::vector<float> memoryList;\par
00045     std::vector<float> ramList;\par
00046 \par
00047     QStringList ramLabels;\par
00048 \par
00049     std::sort(data.begin(), data.end(), sortcol);\par
00050 \par
00051     {\cf19 for} ({\cf18 int} i = 0; i < (int) data.size(); i++)\par
00052     \{\par
00053         {\cf19 if}(std::find(memoryList.begin(), memoryList.end(), data[i][1]) == memoryList.end())\par
00054             memoryList.push_back(data[i][1]);\par
00055 \par
00056         {\cf19 if}(std::find(ramList.begin(), ramList.end(), data[i][0]) == ramList.end())\par
00057         \{\par
00058             ramList.push_back(data[i][0]);\par
00059             ramLabels.push_back(QString::number(data[i][0]));\par
00060         \}\par
00061 \par
00062     \}\par
00063 \par
00064     std::sort(memoryList.begin(), memoryList.end());\par
00065 \par
00066     {\cf19 for} ({\cf18 int} i = 0; i < (int) memoryList.size(); i++)\par
00067     \{\par
00068         QBarSet *set = {\cf17 new} QBarSet(QString::number(memoryList[i]));\par
00069 \par
00070         {\cf18 float} price = 0;\par
00071 \par
00072         {\cf19 for}({\cf18 int} j = 0; j < (int) ramList.size(); j++)\par
00073         \{\par
00074             {\cf19 for}({\cf18 int} k = 0; k < (int) data.size(); k++)\par
00075             \{\par
00076                 {\cf19 if} (data[k][1] == memoryList[i] && ramList[j] == data[k][0])\par
00077                 \{\par
00078                     price += data[k][2];\par
00079                 \}\par
00080             \}\par
00081 \par
00082             *set << price;\par
00083             price = 0;\par
00084         \}\par
00085 \par
00086         series->append(set);\par
00087     \}\par
00088 \par
00089     QChart *chart = {\cf17 new} QChart();\par
00090     QChartView *chartView = {\cf17 new} QChartView(chart);\par
00091 \par
00092 \par
