00254 \par
00255        {\cf19 for}({\cf18 int} j = 0; j < 11; j++)\par
00256        \{\par
00257            htmlTable += {\cf22 "<td>"};\par
00258            htmlTable += i->second.properties.at(entryProps[j]).c_str();\par
00259            htmlTable += {\cf22 "</td>"};\par
00260        \}\par
00261 \par
00262        htmlTable += {\cf22 "</tr>\\n"};\par
00263     \}\par
00264 \par
00265     htmlTable += {\cf22 "</table>\\n<br>\\n"};\par
00266 \par
00267     QTextDocument document;\par
00268     document.setHtml(htmlTable);\par
00269 \par
